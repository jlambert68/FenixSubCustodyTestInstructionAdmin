{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SendMT542 Request, version 1.0",
  "type": "object",
  "properties": {
    "TestInstructionVersion": {
      "type": "string",
      "enum": ["v1.0"]
    },
    "TestCaseExecutionUuid": {
      "type": "string",
      "format": "uuid"
    },
    "TestInstructionExecutionUuid": {
      "type": "string",
      "format": "uuid"
    },
    "TestInstructionExecutionVersion": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "ExpectedToBePassed": {
      "type": "string",
      "enum": ["true", "false"]
    }
  },
  "required": ["TestInstructionVersion",
    "TestCaseExecutionUuid",
    "TestInstructionExecutionUuid",
    "TestInstructionExecutionVersion",
    "ExpectedToBePassed"],
  "additionalProperties": false
}
